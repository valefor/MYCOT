[alias]
    st = status
    ci = commit
    co = checkout
    di = diff
    br = branch
    dog = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    dag = log --oneline --graph

[user]
	name = Adrian Hu
	email = adrian.hu01@sap.com

[core]
    editor = vim

[color]
    status = auto
    branch = auto
    diff = auto
    ui = true
    pager = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
